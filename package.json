{
  "name": "@rolimoa/workspace",
  "private": true,
  "workspaces": [
    "server",
    "client"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "serve": "run-p serve:server serve:client",
    "serve:server": "npm run serve --workspace=server",
    "serve:client": "npm run serve --workspace=client",
    "preinstall": "npx only-allow npm"
  },
  "devDependencies": {
    "npm-run-all2": "^7.0.2"
  }
}
